{
  "extends": "tsconfig/tsconfig.base.node.json",
  "include": [
    "./src/**/*.ts",
    "./src/**/*.json",
    "./src/**/*.d.ts"
  ],
  "exclude": [
    "./src/**/*.spec.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
}